{
  "cli": {
    "version": ">= 12.6.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "distribution": "internal"
    },
    "arm64": {
      "android": {
        "buildType": "apk",
        "architecture": "arm64"
      }
    },
    "armv7": {
      "android": {
        "buildType": "apk",
        "architecture": "armv7"
      }
    },
    "x86": {
      "android": {
        "buildType": "apk",
        "architecture": "x86"
      }
    },
    "production": {
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
